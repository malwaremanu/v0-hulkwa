{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "HulkWa Bulk WA Sender - Excel, Google Sheets",
  "version": "5.76.1001",
  "description": "Send personalised WhatsApp messages from Google Sheets, Excel Sheets. Send images and documents. Download contacts and more.",
  "permissions": [
    "storage",
    "contextMenus",
    "tabs",
    "power"
  ],
  "short_name": "HulkWa",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_start",
      "world": "ISOLATED"
    },
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "lib.js"
      ],
      "run_at": "document_idle",
      "world": "MAIN"
    },
    {
      "matches": [
        "https://*.HulkWa.com/*"
      ],
      "js": [
        "webContentScript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/lib.js",
        "/assets/*"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ],
  "manifest_version": 3
}